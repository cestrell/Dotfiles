#!/bin/bash

read -r -d '' rose << EOM
\e[38;5;161m       .--.---.
      ( \\'--'/ )
     ( '..-...' )
      '.'-._.'.'
\e[38;5;42m       <'-.,.->
          \`\\(   _
          < \\ / \\
          __ \\\\|_/
         /  \\ \\\\
          '--'.\\ >
                \\\\\e[0m
EOM

printf "\n%b\n\n" "$rose"
